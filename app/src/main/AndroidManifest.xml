<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools">

    <application
            android:allowBackup="true"
            android:dataExtractionRules="@xml/data_extraction_rules"
            android:fullBackupContent="@xml/backup_rules"
            android:icon="@drawable/baseline_tips_and_updates_24"
            android:label="@string/app_name"
            android:roundIcon="@drawable/baseline_tips_and_updates_24"
            android:supportsRtl="true"
            android:theme="@style/Theme.SanPablo_Raposo_SantiagoFrancisco_ProyectoMoviles"
            tools:targetApi="31">
        <activity
                android:name=".FormularioActivity"
                android:exported="false"/>
        <activity
                android:name=".ContactoActivity"
                android:exported="false"/>
        <activity
                android:name=".RVActivity"
                android:exported="false"/>
        <activity
                android:name=".MainActivity"
                android:exported="true"
                android:theme="@style/Theme.SanPablo_Raposo_SantiagoFrancisco_ProyectoMoviles.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>